    DC.initServicesCartBtn = function() {
      var cart = $.jStorage.get('subscription-cart') || {},
          $cart = $('.user_task_bar a.cart'),
          DCLocalStorage = $.jStorage.get('DC') || {},
          countryCode = DCLocalStorage.countryCode;
      if (cart.hasOwnProperty('cameras')) {
        var itemsCount = _.keys(cart.cameras).length;
        if (itemsCount > 0) {
          var url = '/' + countryCode + '/plans/cart';
          $cart.attr('href', url);
          $cart.find('.circle .number').html(itemsCount);
          $cart.show();
        }
      }else {
        $cart.hide();
      }
    }

